<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:jci='urn:import:com.jci.client.application.ui'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <g:HTMLPanel addStyleNames="{res.style.tuile}">
        <g:SimplePanel ui:field="header"/>
        <g:HTMLPanel ui:field="main"/>
        <jci:SponsorsView ui:field="sponsors"/>
        <div class="{res.style.clear}"/>
        <div class="{res.style.footer}">
            <jci:FooterView ui:field="footer"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
