<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <ui:with field="mainRes" type="com.jci.client.resource.main.MainResource"/>
    <g:HTMLPanel>
        <div class="{res.style.width} {mainRes.style.dragUp}">
            <div class="{mainRes.style.sliderWrapper} {res.style.round}">
                <div class="{mainRes.style.sliderPhoto}">
                    <div class="{mainRes.style.aWonderfulCity}">
                        <p class="bebas">... a wonderful city</p>
                    </div>
                    <div class="{mainRes.style.button_visio}">
                        <g:Button addStyleNames="{mainRes.style.buttons_visio_off}"></g:Button>
                        <g:Button addStyleNames="{mainRes.style.buttons_visio_off}"></g:Button>
                        <g:Button addStyleNames="{mainRes.style.buttons_visio_on}"></g:Button>
                        <g:Button addStyleNames="{mainRes.style.buttons_visio_off}"></g:Button>
                        <g:Button addStyleNames="{mainRes.style.buttons_visio_off}"></g:Button>
                    </div>
                </div>
                <div class="{mainRes.style.sliderButtons}">
                    <div>
                        <p>Register
                            <br/>
                            NOW!
                        </p>
                    </div>

                    <div>
                        <p>Getting to
                            <br/>
                            Quebec City
                        </p>
                    </div>

                    <div>
                        <p>Some good
                            <br/>
                            Hotels
                        </p>
                    </div>

                    <div>
                        <p>Look at the
                            <br/>
                            Programs
                        </p>
                    </div>
                </div>

            </div>

            <div class="{mainRes.style.sponsorWrapper} {res.style.round}">

            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
