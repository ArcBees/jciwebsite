<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <ui:with field="registerRes" type="com.jci.client.resource.register.RegisterResource"/>
    <g:HTMLPanel>
        <div class="{res.style.width} {res.style.white} {res.style.dragUp} {res.style.round}
        {res.style.content}">
            <div ui:field="sliderButtons" class="{res.style.actions} carousel-indicators">
                <a class="{res.style.button_gray} {res.style.active}" data-target="#myCarouselProgram" data-slide-to="0">
                    18 september
                </a>
                <a class="{res.style.button_gray}" data-target="#myCarouselProgram" data-slide-to="1">
                    19 september
                </a>
                <a class="{res.style.button_gray}" data-target="#myCarouselProgram" data-slide-to="2">
                    20 september
                </a>
                <a class="{res.style.button_gray}" data-target="#myCarouselProgram" data-slide-to="3">
                    21 september
                </a>
            </div>

            <h1>Program (Work in progress)</h1>
            <div class="carousel slide" id="myCarouselProgram">
                <div class="carousel-inner">
                    <div class="item active">1</div>
                    <div class="item">2</div>
                    <div class="item">3</div>
                    <div class="item">4</div>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
