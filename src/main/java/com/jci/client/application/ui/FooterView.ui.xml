<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <ui:with field="footRes" type="com.jci.client.resource.footer.FooterResource"/>
    <ui:with field="sponsRes" type="com.jci.client.resource.footer.sponsors.SponsorsResource"/>
    <g:HTMLPanel>
        <div class="{res.style.width} {sponsRes.style.sponsorWrapper} {res.style.round} {res.style.shadow}">
            <a href="http://www.arcbees.com" target="_blank">
                <g:Image resource="{sponsRes.arcbees}" altText="ArcBees"/>
            </a>
            <a href="http://www.qvc.qc.ca" target="_blank">
                <g:Image resource="{sponsRes.qvc}" altText="QVC"/>
            </a>
            <a href="http://www.lavery.ca/" target="_blank">
                <g:Image resource="{sponsRes.lavery_en}" altText="Lavery"/>
            </a>
        </div>

        <div class="{res.style.width} {footRes.style.city}">
            <div class="{res.style.round} {res.style.white} {footRes.style.social} {res.style.shadow} bebas">
                <a href="https://www.facebook.com/Unlimited2013" target="_blank"
                   class="{footRes.style.follow}">Follow Us</a>
                <a href="javascript:" class="{footRes.style.share}">Share</a>
            </div>

            <h2>2013 JCI Canada National Convention</h2>
            <p>
                September 18th to 21st, Hosted by JCI Québec
            </p>

            <div  class="{footRes.style.location}">
                <p>Hotel Pur,
                    <a href="tel:18004683261">1-800-468-3261</a>
                </p>
                <p>
                    <a href="https://maps.google.ca/maps?q=395+Couronne+Street,+Qu%C3%A9bec+(QC),+G1K+7X4&amp;hl=en&amp;authuser=0&amp;ie=UTF-8&amp;ei=ZcFIUfJe16ngA_DSgegN&amp;ved=0CAsQ_AUoAg">395 Couronne Street, Québec (QC), G1K 7X4</a>
                </p>
            </div>
        </div>
        <div class="{footRes.style.copy}">
            <div class="{res.style.width}">
                <p>
                    © JCI Quebec 2013. All rights reserved. This website is a courtesy of
                    <a href="http://www.arcbees.com" target="_blank">ArcBees</a>.
                </p>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
