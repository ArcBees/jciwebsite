<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <ui:with field="headRes" type="com.jci.client.resource.header.HeaderResource"/>
    <g:HTMLPanel addStyleNames="{headRes.style.bottomBorder} {headRes.style.rootWrapper} {res.style.white}">
        <div class="{res.style.width}">
            <g:Image resource="{headRes.headLangue_EN}" addStyleNames="{headRes.style.headLangue}"/>
            <div class="{headRes.style.left}">
                <g:Image resource="{headRes.headLogoJCI_EN}" addStyleNames="{headRes.style.logoMargin}"/>
            </div>
            <div class="{headRes.style.right} {headRes.style.rightColumn}">
                <div class="{headRes.style.title}">2013 JCI Canada National Convention</div>
                <div class="{headRes.style.subtitle}">September 18th to 21st, Hosted by JCI Quebec</div>
                <div class="{headRes.style.blackbar}"/>
                <div class="{headRes.style.right} {headRes.style.navbar} bebas">
                    <a href="#home" class="{headRes.style.active}">Home</a>
                    <a href="#register">Register</a>
                    <a>Programs</a>
                    <a>Partners</a>
                    <a>About</a>
                    <a>Media</a>
                    <a>Contact</a>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
