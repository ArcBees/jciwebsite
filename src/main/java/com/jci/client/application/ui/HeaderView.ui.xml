<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <ui:with field="headRes" type="com.jci.client.resource.header.HeaderResource"/>
    <ui:with field="nameTokens" type="com.jci.client.place.NameTokens"/>

    <g:HTMLPanel addStyleNames="{headRes.style.bottomBorder} {headRes.style.rootWrapper} {res.style.white}">
        <div class="{res.style.width}">
            <g:Image resource="{headRes.headLangue_EN}" addStyleNames="{headRes.style.headLangue}"/>
            <div class="{headRes.style.left}">
                <a href="#{nameTokens.getHome}">
                    <g:Image resource="{headRes.headLogoJCI_EN}" addStyleNames="{headRes.style.logoMargin}"/>
                </a>
            </div>
            <div class="{headRes.style.right} {headRes.style.rightColumn}">
                <div class="{headRes.style.title}">2013 JCI Canada National Convention</div>
                <div class="{headRes.style.subtitle}">September 18th to 21st, Hosted by JCI Quebec City</div>
                <div class="{headRes.style.blackbar}"/>
                <div ui:field="menu" class="{headRes.style.right} {headRes.style.navbar} bebas">
                    <a href="#{nameTokens.getHome}" class="{headRes.style.active} {nameTokens.getHome}">Home</a>
                    <a href="#{nameTokens.getRegister}" class="{nameTokens.getRegister}">Registration</a>
                    <a href="#{nameTokens.getProgram}" class="{nameTokens.getProgram}">Program</a>
                    <a href="#{nameTokens.getAbout}" class="{nameTokens.getAbout}">About</a>
                    <a>Contact</a>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
