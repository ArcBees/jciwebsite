<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales='default'>
    <ui:with field="res" type="com.jci.client.resource.CommonResource"/>
    <ui:with field="sponsRes" type="com.jci.client.resource.sponsors.SponsorsResource"/>

    <g:HTMLPanel>
        <div class="{res.style.width} {sponsRes.style.sponsorWrapper} {res.style.round} {res.style.shadow}">
            <a href="http://www.arcbees.com/" target="_blank">
                <ui:attribute name="href" description="Href > Arcbees"/>
                <g:Image resource="{sponsRes.arcbees}" altText="ArcBees"/>
            </a>
            <a href="http://www.qvc.qc.ca/eng/QVC-01-About-us/QVC-About-us.htm" target="_blank">
                <ui:attribute name="href" description="Href > QVC"/>
                <g:Image resource="{sponsRes.qvc}" altText="QVC"/>
            </a>
            <a href="http://www.lavery.ca/lavery-laywer-firm/" target="_blank">
                <ui:attribute name="href" description="Href > Lavery"/>
                <g:Image resource="{sponsRes.lavery}" altText="Lavery"/>
            </a>
            <a href="http://www.cercledesambassadeurs.com/" target="_blank">
                <g:Image resource="{sponsRes.cercleAmbassadeurs}" altText="Cercle des Ambassadeurs"/>
            </a>
            <a href="http://placestefoy.com/en/" target="_blank">
                <ui:attribute name="href" description="Href > SteFoy"/>
                <g:Image resource="{sponsRes.placeSteFoy}" altText="Place Ste-Foy"/>
            </a>
            <a href="http://www.deloitte.com/view/en_CA/ca/index.htm" target="_blank">
                <ui:attribute name="href" description="Href > Deloitte"/>
                <g:Image resource="{sponsRes.deloitte}" altText="Deloitte"/>
            </a>
            <a href="http://www4.fsa.ulaval.ca/cms/accueil/recherche/chairescentrerecherche/chairesderecherche/stephenjarislowsky" target="_blank">
                <g:Image resource="{sponsRes.chaire}" altText="Chaire Stephen-A.-Jarislowsky"/>
            </a>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
